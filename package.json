{
  "name": "takeat-api",
  "version": "1.0.0",
  "description": "Teste t√©cnico para a Takeat",
  "scripts": {
    "dev": "npm run services:up && ts-node-dev src/app/index.ts",
    "services:up": "docker compose -f src/infra/docker/compose.yaml up -d",
    "services:stop": "docker compose -f src/infra/docker/compose.yaml stop",
    "services:down": "docker compose -f src/infra/docker/compose.yaml down",
    "test": "jest",
    "migrations:up": "npx sequelize-cli db:migrate",
    "migrations:generate": "npx sequelize-cli migration:generate",
    "migrations:undo": "npx sequelize-cli db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GutembergLamark/takeat-api.git"
  },
  "author": "Gutemberg Lamark",
  "bugs": {
    "url": "https://github.com/GutembergLamark/takeat-api/issues"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
